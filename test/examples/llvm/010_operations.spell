# Result: integer, 1

import "assertions"

test#integer () = 
  do
    assert#equal  2 (1 + 1)
    assert#equal  0 (1 + -1)
    assert#equal -2 (-1 + -1)
    assert#equal  3 (1 + 1 + 1)
    assert#equal  1 (3 - 2)
    assert#equal -1 (2 - 3)
    assert#equal  6 (2 * 3)
    assert#equal  2 (4 / 2)
    assert#equal  1 (3 / 2)
		assert#equal  1 ((3 * 2) - 4) / 2
    
main () = 
  do
    test#integer
