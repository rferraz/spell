range (first last) =
  ? first <= last -> range' first last
  ?             _ -> reverse (range' last first)
  with
    range' (first last) =
      ? first <= last -> first : (range' (first + 1) last)
      ?             _ -> []

map (predicate list) = 
  ? empty list -> list
  ?         _ -> (apply predicate (head list)) : (map predicate (tail list))

